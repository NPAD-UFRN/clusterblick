#!/bin/bash

echo $(date "+%d/%m/%y %T") > raw/ssupervisor_squeue1.txt

echo "[squeue] Information about jobs located in the Slurm scheduling queue" >> raw/ssupervisor_squeue1.txt
squeue >> raw/ssupervisor_squeue1.txt
echo $'#  #' >> raw/ssupervisor_squeue1.txt

scp -P 4422 raw/ssupervisor_squeue1.txt adelsonNPAD@10.7.8.74:/var/www/html/protected/tmp/supervisor/raw
